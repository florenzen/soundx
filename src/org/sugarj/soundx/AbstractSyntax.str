/**
 * Signature of SoundX abstract syntax
 */
module org/sugarj/soundx/AbstractSyntax

signature constructors
	/**
	 * Form declaration
	 */
	SXAbsJudgementForm: List(String) -> SXAbsJudgementForm

    /**
     * Types of sorts
     */
    SXAbsSortContextFree: SXAbsSort
    SXAbsSortLexical:     SXAbsSort

    /**
     * Arity declaration
     */
    SXAbsArity: List(String) * String -> SXAbsArity

	/**
	 * Inference rules
	 */
	SXAbsInfRule: List(SXJudgement) -> SXJudgement
