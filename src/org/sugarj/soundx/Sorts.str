module org/sugarj/soundx/processing/Sorts

imports org/sugarj/soundx/AbstractSyntax

strategies
	sx-symbol-to-sort:
		sort(name) -> Some(SXSortSimple(name))
	sx-symbol-to-sort:
		lit(string) -> None()
	sx-symbol-to-sort:
		char-class(string) -> None()